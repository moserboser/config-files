{
    "spacing": 0,

    "modules-left": [
        "sway/workspaces",
        "sway/mode",
        "sway/scratchpad",
        "sway/window"
    ],
    "modules-right": [
        "cpu",
        "memory",
        "backlight",
        "pulseaudio",
        "battery",
        "network",
        "clock",
        "custom/power",
      //"idle_inhibitor",
        "custom/notification",
        "tray",
    ],

    "custom/power": {
        "format": "ÔÄë ",
        "tooltip-format": "power manager",
        "on-click": "wlogout"
    },

    "clock": {
        "interval": 30,
        "format": "{:%a, %d %b %Y | %H:%M %p}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
    },

    "cpu": {
        "interval": 1,
        "format": "CPU: {usage}%",
    },

    "memory": {
        "interval": 1,
        "format": "Mem:{}%"
    },

    "backlight": {
        "format": "{icon} {percent}%",
        "format-icons": ["üîÖ", "üîÜ"]
    },

    "battery": {
        "states": {
            "warning": 30,
            "critical": 15
        },
        "interval": 1,
        "format": "{icon} {capacity}%",
        "format-charging": "‚ö° {capacity}%",
        "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
    },

    "network": {
        "format-wifi": "Ôá´ ({signalStrength}%) {essid}",
        "format-ethernet": "{ipaddr}/{cidr} Ôûñ",
        "tooltip-format": "{ifname} via {gwaddr} Ôûñ",
        "format-linked": "{ifname} (No IP) Ôûñ",
        "format-disconnected": "Disconnected ‚ö†"
    },

    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-bluetooth": "{icon}Ôäî {volume}%",
        "format-muted": "Ôö© {format_source}",
        "format-source": "ÔÑ∞ {volume}%",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "on-click": "pavucontrol"
    },

    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },

    "tray": {
        "spacing": 10
    },

   "custom/notification": {
    "tooltip": false,
        "format": "{icon}",
           "format-icons": {
                     "notification": "Û∞éü",
                     "none": "Û∞éü",
                     "dnd-notification": "Û∞éü",
                     "dnd-none": "Û∞éü",
                     "inhibited-notification": "Û∞éü",
                     "inhibited-none": "Û∞éü",
                     "dnd-inhibited-notification": "Û∞éü",
                     "dnd-inhibited-none": "Û∞éü"
                },
                "return-type": "json",
                "exec-if": "which swaync-client",
                "exec": "swaync-client -swb",
                "on-click": "swaync-client -t -sw",
                "on-click-right": "swaync-client -d -sw",
                "escape": true
    },

}
